def duplicate_encode(word):
    #your code here
    word=word.lower()
    word=list(word)
    nw=""

    for x in range(len(word)):
        #if x==len(word):
        #    print("끝값={}".format(x))
        #    if x in word[]
        word.insert(0,word[x])
        word.pop(x+1)
        print(word)
        if word[0] in word[1:len(word)]:
            nw+=")"
            print(word[x],x)
        else:
            nw+="("
    
    return nw
